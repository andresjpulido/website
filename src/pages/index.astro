---
import Layout from "../layouts/Layout.astro";
import SectionTech from "../components/SectionTech.astro";
import Footer from "../components/Footer.astro";
import SectionProjects from "../components/SectionProjects.astro";
import SectionExperience from "../components/SectionExperience.astro";
import Nav from "../components/Nav.astro";
import Cover from "../components/Cover.astro";
import "../base.css";

const {
  title = "Andres Pulido - Software developer",
  description = "A seasoned software developer with over a decade of experience in the world of coding. I'm deeply passionate about technology and consider myself a lifelong learner, always eager to explore new horizons in the ever-evolving tech landscape.",
  image= "https://andresjpulido.com/images/AndresPulido.png",
  frontmatter,
  robots,
} = Astro.props;

---


<Layout {title} {description} {image} {frontmatter} {robots}>
  <Nav />
  <Cover />
  <SectionProjects />
  <SectionTech />
  <SectionExperience />
  <Footer />
</Layout>

<style>
  body {
    background-color: #f7f7f7;
  }
</style>
